<Project Sdk="Microsoft.NET.Sdk">

  <PropertyGroup>
    <TargetFramework>net8.0</TargetFramework>
	<Description>C# source generators for Sphere10 Framework that generate boilerplate at compile time. Includes generator implementations and shared helper code to support framework conventions and improve developer ergonomics.</Description>
    <PackageTags>sphere10 framework code-generation source-generator</PackageTags>
    <ImplicitUsings>enable</ImplicitUsings>
    <Nullable>enable</Nullable>
  </PropertyGroup>

</Project>
